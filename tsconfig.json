{
    "compilerOptions": {
        "sourceMap": true,
        "noImplicitAny": false,
        "module": "esnext",
        "target": "es6",
        "jsx": "react",
        "strict": true,
        "removeComments": false,
        "moduleResolution": "node",
        "preserveConstEnums": true,
        "baseUrl": "./",
        "typeRoots" : ["./typings"],
        "types": [
            "@types/jest"
        ],
        "paths": {
            "utility": ["scripts/utility"],
            "const" : ["scripts/const"],
            "models" : ["scripts/models"],
            "@types/*": ["scripts/types/*"]
        }
    }
}